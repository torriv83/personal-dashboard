APP_NAME=Laravel
APP_ENV=production
# Security: Generate a unique key with 'php artisan key:generate' - never use an empty key in production
APP_KEY=
# Security: Set to false in production to prevent exposing sensitive information
# Only set to true in local development environments for debugging
APP_DEBUG=false
APP_URL=http://localhost

APP_LOCALE=nb
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=nb_NO

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
# Log levels: debug, info, notice, warning, error, critical, alert, emergency
# Use 'warning' or higher in production, 'debug' for local development
LOG_LEVEL=warning

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=personal_dashboard
DB_USERNAME=root
# Security: Use a strong, unique password for database access in production
DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
# Security: Consider setting to true in production for additional session data encryption
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
# Security: Set a strong password for Redis in production (never use null)
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mail Configuration (use 'brevo' in production, 'log' for local)
MAIL_MAILER=log
MAIL_FROM_ADDRESS="noreply@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# Brevo API (for production email sending)
# Security: Replace with your actual API key - never commit real keys to version control
BREVO_API_KEY=your-brevo-api-key

# Security: AWS credentials - use IAM roles where possible, never commit actual credentials
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# SFTP Backup Configuration
# Security: Use strong credentials and consider using SSH keys instead of passwords
SFTP_HOST=
SFTP_USERNAME=
SFTP_PASSWORD=
SFTP_PORT=
SFTP_ROOT=

# Backup Settings
# Security: Keep webhook URLs private to prevent unauthorized notifications
DISCORD_WEBHOOK=

# Security: Keep your YNAB API token secret - never share or commit actual tokens
YNAB_TOKEN=
YNAB_BUDGET_ID=

# Web Push Notifications (VAPID)
# Generate keys with: npx web-push generate-vapid-keys
# Security: Keep VAPID_PRIVATE_KEY secret - only VAPID_PUBLIC_KEY should be exposed to clients
VAPID_SUBJECT=mailto:your-email@example.com
VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=

# Security: Replace with your actual API key - keep it secret and rotate regularly
OPENROUTESERVICE_API_KEY=din-api-n√∏kkel-her

VITE_APP_NAME="${APP_NAME}"
